<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:OrganizeMySelf.Views"
                    xmlns:vm="clr-namespace:OrganizeMySelf.ViewModels">

    <DataTemplate DataType="{x:Type vm:StorageViewModel}">
        <local:StorageView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:StorageAddViewModel}">
        <local:StorageAddView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:StorageEditViewModel}">
        <local:StorageEditView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AnalizzaDatiViewModel}">
        <local:AnalizzaDatiView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CategoriaViewModel}">
        <local:CategoriaView />
    </DataTemplate>
</ResourceDictionary>